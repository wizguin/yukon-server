  "bugs": {
    "url": "https://github.com/wizguin/yukon-server/issues"
  },
  "homepage": "https://github.com/wizguin/yukon-server#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-watch": "^7.7.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "fastest-validator": "^1.13.0",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "pm2": "^5.2.0",
    "rate-limiter-flexible": "^2.3.8",
    "sequelize": "^5.22.5",
    "socket.io": "^4.5.1",
    "uuid": "^8.3.2"
  },
